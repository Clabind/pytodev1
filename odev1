a = int(input("Birinci sayıyı giriniz:"))
b = int(input("İkinci sayıyı giriniz:"))
kod=input("İşlem kodları şunlardır:\n1-)Toplama\n2-)Çıkarma\n3-)Çarpma\n4-)Bölme\n5-)Mod")
if(kod==1):
  print (a+b)
elif(kod==2):
  print (a-b)
elif(kod==3):
  print (a*b)
elif(kod==4):
  print(a/b)
elif(kod==5):
  print(a%b)
else:
  print("İşlem kodu uygun değil!")
